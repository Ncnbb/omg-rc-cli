{
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType":"module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "no-console": "off",
    "strict": ["error", "global"],
    "curly": "warn",
    // "quotes": [1, "double"], // 双引号
    "no-unused-vars": "off" // 只声明没使用的变量不报错
  }
}

